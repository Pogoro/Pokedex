    <!--The email form
    <div class="contact">
        <h1>Contact Us</h1>
        <div class="form-group">
            <label for="exampleFormControlInput1">Send us a message!</label>
            <input type="email" class="form-control" id="exampleFormControlInput1" placeholder="name@example.com">
        </div>
        <div class="form-group">
            <label for="exampleFormControlTextarea1"></label>
            <textarea class="form-control" id="exampleFormControlTextarea1" rows="3" placeholder="Type message here..."></textarea>
        </div>
        <button type="submit" class="contact-me-submit">Submit</button>
    </div>
    -->
    <div class="contact-form">
        <h1>Contact Us</h1>
        <div class="txtb">
            <label>Full Name: </label>
            <input type="text" name="" value="" placeholder="Name...">
        </div>
        <div class="txtb">
            <label>Email: </label>
            <input type="email" name="" value="" placeholder="Email...">
        </div>
        <div class="txtb">
            <label>Message: </label>
            <textarea name="" id="" cols="30" rows="10" placeholder="Type message here..."></textarea>
        </div>
        <a class ="btn">Send</a>
    </div>

    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
